{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "productimg": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    },
    "producttitle": {
      "type": "string",
      "required": true
    },
    "productprice": {
      "type": "decimal",
      "required": true
    },
    "discription": {
      "type": "text",
      "required": true
    },
    "rating": {
      "type": "decimal"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "LAPTOP",
        "PHONE"
      ],
      "required": true
    }
  }
}
